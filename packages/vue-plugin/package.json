{
  "name": "@bicou/prosemirror-render-vue",
  "version": "0.0.13",
  "description": "Vue plugin to render ProseMirror JSON data",
  "license": "MIT",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbicou/prosemirror-render.git",
    "directory": "packages/vue-plugin"
  },
  "homepage": "https://gbicou.github.io/prosemirror-render/vue-plugin/",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc && vite build",
    "dev": "vite build --watch",
    "lint": "eslint .",
    "lint:sarif": "eslint -f @microsoft/eslint-formatter-sarif -o eslint.sarif .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "change-case": "5.4.4",
    "defu": "6.1.4"
  },
  "peerDependencies": {
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@bicou/eslint-config": "workspace:*",
    "@types/node": "22.13.5",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/coverage-v8": "3.0.6",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.21.0",
    "happy-dom": "17.1.3",
    "rollup-plugin-node-externals": "8.0.0",
    "typescript": "5.6.3",
    "vite": "6.1.0",
    "vite-plugin-dts": "4.5.0",
    "vitest": "3.0.6",
    "vitest-github-actions-reporter": "0.11.1",
    "vue-tsc": "2.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
