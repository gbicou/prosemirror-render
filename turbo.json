{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".nuxt/**",
        ".output/**",
        ".vercel/**",
        "dist/**",
        ".vitepress/cache",
        ".vitepress/dist"
      ]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": ["VERCEL", "GITHUB_ACTIONS"]
}
